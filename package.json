{
  "name": "portfolio-timds-design",
  "version": "1.0.0",
  "description": "My portfolio website 2021, made with 11ty",
  "main": "index.js",
  "scripts": {
    "eleventy": "npx @11ty/eleventy",
    "sass": "sass --style=compressed src/style/main.scss _site/main.css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*",
    "build": "npm run eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgm-timdesae/portfolio-timds-design.git"
  },
  "keywords": [
    "Eleventy",
    "portfolio"
  ],
  "author": "Tim De Saeger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pgm-timdesae/portfolio-timds-design/issues"
  },
  "homepage": "https://github.com/pgm-timdesae/portfolio-timds-design#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "npm-run-all": "^4.1.5"
  }
}
